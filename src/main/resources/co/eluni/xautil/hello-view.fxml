<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<VBox alignment="TOP_CENTER" spacing="5.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="co.eluni.xautil.HelloController">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>

    <Label fx:id="cwdLabel"/>
    <Button text="Choose Directory" onAction="#chooseDirectory"/>

    <TableView fx:id="mainTable"/>
</VBox>
